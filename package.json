{
  "name": "lcpmobileapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "startclear": "expo start --clear",
    "startaemu": "cmd /c \"cd scripts && startaemu.bat\"",
    "startaemuclb": "cmd /c \"cd scripts && startaemuclb.bat\"",
    "stopaemu": "cmd /c \"cd scripts && stopaemu.bat\"",
    "build": "eas build -p android --profile preview",
    "buildrun": "eas build:run -p android",
    "prebuild": "expo prebuild",
    "prebuildandroid": "expo prebuild --platform android",
    "prebuildios": "expo prebuild --platform ios",
    "android": "expo run:android",
    "androidrn": "npx react-native run-android --mode=\"release\"",
    "ios": "expo run:ios",
    "iosrn": "npx react-native run-ios --mode=\"release\"",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/material-bottom-tabs": "^6.2.28",
    "@react-navigation/native": "^6.0.2",
    "dotenv": "^16.4.5",
    "expo": "~51.0.31",
    "expo-constants": "~16.0.1",
    "expo-font": "~12.0.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
